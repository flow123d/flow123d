{
  problem = {
    TYPE = "SequentialCoupling", 
    description = "Simple dual porosity test - steady flow, simple transport",
    mesh = {
      mesh_file = "input/dual_por.msh"
    }, 
    
    primary_equation = {
      TYPE = "Steady_MH",
      
      data = [
        { region = "volume", 
          conductivity = 1e-20
        },
        { r_set = "BOUNDARY",
          bc_type = "dirichlet",
          bc_pressure = { TYPE="FieldFormula", value = "1"}
        }
      ],
       
      output = {
        output_stream = {
          REF = "/output_streams/0"
        }, 
        output_fields = [ "pressure_p0", "pressure_p1", "velocity_p0" ]
      }, 

      solver = { 
        TYPE = "Petsc",
        a_tol = 1e-7 
      }
    }, 
    
    secondary_equation = {
      TYPE = "TransportOperatorSplitting",
 
      data = [
        { r_set = "ALL",
          init_conc = 1.0,
          por_m = 0.25 
        },
        { r_set = "BOUNDARY",
          bc_conc = {TYPE="FieldFormula", value = "0"}
        }
      ],
      
      substances = [
        "A",
        "B"
        ],
      
      output_stream = {
        REF = "/output_streams/1"
      }, 
      time = {
        end_time = 5.0
      },
      
      reactions = {
        TYPE = "DualPorosity",
        
        substances = {
          REF = "/problem/secondary_equation/substances"
        },
        
        data = [
          {
            r_set="ALL",
            alpha = [0.01,0.01],
            immob_porosity = 0.25,
            init_conc_immobile = [0.0, 0.0]
          }
        ],
        output_fields = []
      }
    } 
  }, 
  
    output_streams = [
      {
        file = "dual_por.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        name = "flow_output_stream"
      }, 
      {
        file = "dual_por_transport.pvd", 
        format = {
          TYPE = "vtk", 
          variant = "ascii"
        }, 
        time_step = 0.5,
        name = "transport_output_stream"
      }
    ]
 

}